	"devDependencies": {
		"concurrently": "^5.2.0",
		"create-react-app": "^3.4.1",
		"nodemon": "^2.0.4"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"express": "^4.17.1",
		"if-env": "^1.0.4",
